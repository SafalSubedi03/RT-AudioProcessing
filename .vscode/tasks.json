{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build ANC Project",
            "type": "shell",
            "command": "${workspaceFolder}/AdaptiveNoiseCancellation_Cpp/main/build_ANC.bat",
            "options": {
                "cwd": "${workspaceFolder}/AdaptiveNoiseCancellation_Cpp/main"
            },
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": ["$msCompile"]
        },
        {
            "label": "Build Learn Project",
            "type": "shell",
            "command": "${workspaceFolder}/AdaptiveNoiseCancellation_Cpp/learn/build_Learn.bat",
            "options": {
                "cwd": "${workspaceFolder}/AdaptiveNoiseCancellation_Cpp/learn"
            },
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": ["$msCompile"]
        }
    ]
}
